{{#each photos}}
  <div style="background-image:url({{this.src}})" data-xbig-src="{{this.xbigSrc}}">
    <span>
      <span class="external_link">
        {{this.title}}
        {{#if this.xbigSrc}}
          <i class="fa fa-external-link"></i>
        {{/if}}
      </span>
    </span>
  </div>
{{/each}}
